    "noip",
    "auto",
    "update",
    "confirm"
  ],
  "author": "Conrad Petrich",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/node-cron": "^2.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^9.0.2",
    "node-cron": "^3.0.0"
  }
